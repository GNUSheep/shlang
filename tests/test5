struct Foo {
    x: float,
    y: int,
}

fn main() {
    var p: Foo = help()

    var l: Foo = p

    println(l.x)

    l.x = 81.5

    println(l.x, " ", p.x)
}

fn help() Foo {
    
    var z: Foo = {8.5, 10}

    var x: Foo = {2.5, 4}

    var u: Foo = {12.5, 10}

    var k: String = "MDF"

    var p: Foo = {40.5, 10}

    return x
}
